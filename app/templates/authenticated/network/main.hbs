<BsForm
  @formLayout="vertical"
  @model={{this.netmain.model}}
  @onSubmit={{this.onSubmit}}
  {{on "change" this.netmain.onChange}}
  as |form|
>
  <Header @onSubmit={{form.submit}} @model={{this.netmain}} />
  <SettingsHeader
    @icon="wifi"
    @title={{format-message "Wi-Fi"}}
    @description={{format-message
      "The main Wi-Fi networks provided by your device"
    }}
  />
  <form.element @controlType="text" @property="ipv4.subnet" as |el|>
    <el.control
      placeholder={{format-message "Subnet"}}
      required
      maxlength="32"
    />
  </form.element>
  <SettingsDescription>
    {{format-message "The subnet used by your main wifi"}}
  </SettingsDescription>
</BsForm>