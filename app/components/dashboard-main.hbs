<div>
  <DashboardHeader @title="Main Network" @car={{@car}} />
  <BsListGroup class="mb-3" as |ListGroup|>
    <ListGroup.item>
      <div class="container">
        <div class="row align-items-start align-items-center d-flex">
          <div class="col-4 text-end mw-50">
            <BsIcon
              @name={{wifiicon this.model.device.wifi.signal}}
              class="text-primary h1"
            />
          </div>
          <div class="col capitalize-first-letter">
            <div class="container">
              <div class="row align-items-start d-flex">
                <div class="col-4 text-end mw-50">
                  SSID
                </div>
                <div class="col-4 text-start mw-50">
                  {{this.model.device.wifi.ssid}}
                </div>
              </div>
              <div class="row align-items-start d-flex">
                <div class="col-4 text-end mw-50">
                  Channel
                </div>
                <div class="col-4 text-start mw-50">
                  36
                </div>
              </div>
              <div class="row align-items-start d-flex">
                <div class="col-4 text-end mw-50">
                  Signal
                </div>
                <div class="col-4 text-start mw-50">
                  {{this.model.device.wifi.signal}}
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </ListGroup.item>
  </BsListGroup>
  <BsListGroup @flush={{true}} class="mb-5" as |ListGroup|>
    <ListGroupItem @parent={{ListGroup}} @key="SSID" @value="MyHome" />
    <ListGroupItem @parent={{ListGroup}} @key="Channel" @value="36" />
  </BsListGroup>
  <BsListGroup as |ListGroup|>
    <ListGroupNav
      @parent={{ListGroup}}
      @route="authenticated.qrcode.main"
      @icon="qr-code"
    >QR Code</ListGroupNav>
    <ListGroupNav
      @parent={{ListGroup}}
      @onClick={{@onDevices}}
      @icon="pc-display-horizontal"
    >Connected Devices</ListGroupNav>
    <ListGroupNav
      @parent={{ListGroup}}
      @onClick={{@onUsers}}
      @icon="people"
    >Users</ListGroupNav>
    <ListGroupNav
      @parent={{ListGroup}}
      @onClick={{fn @onManaged "list"}}
      @icon="router"
    >Managed Devices</ListGroupNav>
    <ListGroupNav
      @parent={{ListGroup}}
      @route="authenticated.settings.wifi.main"
      @icon="gear"
    >Configure</ListGroupNav>
    <ListGroupNav
      @parent={{ListGroup}}
      @route="authenticated.settings.wifi.mesh"
      @icon="bezier"
    >Mesh Backhaul</ListGroupNav>

  </BsListGroup>
</div>